{
  "name": "brello",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "packageManager": "yarn@1.22.17",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint:css": "stylelint \"**/*.css\"",
    "generate": "yarn hygen generate",
    "new:component": "yarn hygen new component",
    "update-types": "npx supabase gen types typescript --project-id ddjirrggtysituolvxws > src/shared/api/supabase.ts",
    "gen:sprite": "yarn sprite --group --root src/shared/ui/icon/assets -o public/sprites -d src/shared/ui/icon/sprite.h.ts --reset-unknown-colors",
    "lint:js": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@supabase/supabase-js": "^2.26.0",
    "atomic-router": "^0.9.1",
    "atomic-router-react": "^0.9.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "effector": "^23.0.0",
    "effector-react": "^23.0.1",
    "history": "^5.3.0",
    "patronum": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@neodx/svg": "^0.6.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-effector": "^0.12.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "hygen": "^6.2.11",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "stylelint": "^16.0.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^35.0.0",
    "supabase": "^1.86.2",
